{
  "homepage": "https://github.com/jrc03c/tf-kmeans#readme",
  "name": "@willfind/tf-kmeans",
  "description": "my silly attempt to implement k-means in tensorflow.js",
  "author": "jrc03c",
  "version": "0.0.2",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jrc03c/tf-kmeans.git"
  },
  "main": "src/index.js",
  "scripts": {
    "build": "esbuild src/index.js --bundle --outfile=dist/tf-kmeans.js --minify",
    "watch": "esbuild src/index.js --bundle --outfile=dist/tf-kmeans.js --minify --watch",
    "build-browser-tests": "esbuild tests/browser/src/tests.js --bundle --outfile=tests/browser/tests-bundle.js",
    "watch-browser-tests": "esbuild tests/browser/src/tests.js --bundle --outfile=tests/browser/tests-bundle.js --watch",
    "build-browser-tests-worker": "esbuild tests/browser/src/tests-worker.js --bundle --outfile=tests/browser/tests-worker-bundle.js",
    "watch-browser-tests-worker": "esbuild tests/browser/src/tests-worker.js --bundle --outfile=tests/browser/tests-worker-bundle.js --watch",
    "test": "jest --testTimeout=300"
  },
  "dependencies": {
    "@jrc03c/js-math-tools": "github:jrc03c/js-math-tools",
    "@tensorflow/tfjs": "^4.1.0",
    "@tensorflow/tfjs-node": "^4.2.0"
  },
  "devDependencies": {
    "@jrc03c/bee": "^0.0.18",
    "@jrc03c/js-convex-hull": "github:jrc03c/js-convex-hull",
    "@jrc03c/js-data-science-helpers": "github:jrc03c/js-data-science-helpers",
    "@jrc03c/pause": "^0.0.3",
    "@jrc03c/progress": "github:jrc03c/progress",
    "esbuild": "^0.15.9",
    "eslint": "^8.24.0",
    "jest": "^29.3.1",
    "prettier": "^2.8.1"
  }
}
